{
  "name": "gamifyjs",
  "version": "0.0.1",
  "description": "A 2D side scroller RPG powered by Phaser3, where the core game mechanic is JavaScript",
  "main": "app.js",
  "scripts": {
    "test": "jest",
    "build:prod": "cross-env NODE_ENV=production webpack --mode production",
    "build:dev": "cross-env NODE_ENV=development webpack --mode development",
    "start:dev": "npm-run-all --parallel build:dev lift:dev",
    "lift:dev": "nodemon sails lift",
    "lift": "sails lift"
  },
  "author": "Samuel Chen",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-for-of": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/standalone": "^7.2.5",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.9.0",
    "eslint-config-google": "^0.11.0",
    "eslint-loader": "^2.1.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "phaser": "^3.15.1",
    "ramda": "^0.26.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "source-map": "^0.7.3",
    "source-map-loader": "^0.2.4",
    "stacktracey": "^1.2.106",
    "style-loader": "^0.23.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "@sailshq/lodash": "^3.10.3",
    "sails": "^1.1.0",
    "sails-hook-orm": "^2.1.1"
  }
}
